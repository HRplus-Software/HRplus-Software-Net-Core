<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WebApp.styles.css" asp-append-version="true" />
	<link href="@Url.Content("~/lib/kendo-ui/styles/default-ocean-blue.css")" rel="stylesheet" type="text/css" />
	<script src="@Url.Content("~/lib/jquery/dist/jquery.js")"></script>
	<script src="https://unpkg.com/jszip/dist/jszip.min.js"></script>
	<script src="@Url.Content("~/lib/kendo-ui/js/kendo.all.min.js")"></script>
	<script src="@Url.Content("~/lib/kendo-ui/js/kendo.aspnetmvc.min.js")"></script>
</head>
<body>
    @RenderBody()
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>